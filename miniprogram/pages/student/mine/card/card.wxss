
.cu-card>.cu-item {
  background-color: transparent;
}

.gym-card {
  display: block;
  overflow: hidden;
}

.gym-card>.gym-item {
  display: block;
  background-color: transparent;
  overflow: hidden;
  border-radius: 32rpx;
  margin: 30rpx;
}

.gym-card.case .image .cu-bar {
	position: absolute;
	bottom: 8rpx;
  width: 100%;
	background-color: transparent;
	padding: 0rpx 30rpx;
}

.gym-card.case .image .cu-tag {
	position: absolute;
  right: 0;
  left:41%;
  top: 30rpx;
  width: 100%;
  transform:rotate(45deg);
}

.image>image{
  border-radius: 36rpx;
}

.gym-card.case .image {
  position: relative;
}

.gym-card.case .image image {
  width: 100%;
  height: 360rpx;
}

.bg-shadeBottom-msk {
  background-image: linear-gradient(rgba(0, 0, 0, 0.01), rgba(0, 0, 0, 0.5));
  color: var(--white);
  border-radius: 0rpx 0rpx 36rpx 36rpx;
}

.imagesize {
  margin-top: 10%;
  width: 46%;
  height: 80%;
}

.gray-img { 
  -webkit-filter: grayscale(100%);
  -moz-filter: grayscale(100%);
  -ms-filter: grayscale(100%);
  -o-filter: grayscale(100%);
  filter: grayscale(100%);
  filter: gray;
}
