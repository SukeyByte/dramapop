@import "/colorui/main.wxss";
@import "/colorui/icon.wxss";
@import "/colorui/imIcom.wxss";

.nav-li-order {
  padding: 30rpx;
  border-radius: 48rpx;
  width: 100%;
  margin-top: 40rpx;
  background-image: url(https://6d61-mail-stg-oboxi-1300202136.tcb.qcloud.la/cardBg.png?sign=8b7dba64360983b404dfe4d6a5081b95&t=1608816116);
  background-size: cover;
  background-position: center;
  position: relative;
  z-index: 1;
}

.nav-name-order {
  font-size: 30rpx;
  text-transform: Capitalize;
  margin-top: 20rpx;
  position: relative;
  margin-left: 20rpx;
}

.nav-name-order::before {
  content: "";
  position: absolute;
  display: block;
  width: 40rpx;
  height: 8rpx;
  background: #fff;
  bottom: 0;
  right: 0;
  opacity: 0.5;
}

.nav-name-order::after {
  content: "";
  position: absolute;
  display: block;
  width: 100rpx;
  height: 4rpx;
  background: #fff;
  bottom: 0;
  right: 40rpx;
  opacity: 0.3;
}

.nav-name-order::first-letter {
  font-size: 48rpx;
  margin-right: 1px;
}

.nav-li-price {
  position: absolute;
  right: 130rpx;
  top: 45rpx;
  font-size: 52rpx;
  width: 60rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
}

.selectborder {
  border: 2px solid black;
  border-radius: 48rpx;
  padding: 0;
  height: 420rpx;
}

.bg-shadeBottom-msk {
  background-image: linear-gradient(rgba(0, 0, 0, 0.01), rgba(0, 0, 0, 0.5));
  color: var(--white);
}

.imagesize {
  margin-top: 10%;
  width: 46%;
  height: 80%;
}


.cu-card>.cu-item {
  background-color: transparent;
}

.gym-card {
  display: block;
  overflow: hidden;
}

.gym-card>.gym-item {
  display: block;
  background-color: transparent;
  overflow: hidden;
  border-radius: 48rpx;
  margin: 30rpx;
}

.gym-card.case .image .cu-bar {
  position: absolute;
  bottom: 8rpx;
  width: 100%;
  background-color: transparent;
  padding: 0rpx 30rpx;
}

.gym-card.case .image .cu-tag {
  position: absolute;
  right: 0;
  left: 41%;
  top: 30rpx;
  width: 100%;
  transform: rotate(45deg);
}

.image>image {
  border-radius: 48rpx;
}

.gym-card.case .image {
  position: relative;
}

.gym-card.case .image image {
  width: 100%;
}

.bg-shadeBottom-msk {
  background-image: linear-gradient(rgba(0, 0, 0, 0.01), rgba(0, 0, 0, 0.5));
  color: var(--white);
  border-radius: 0rpx 0rpx 48rpx 48rpx;
}